{
    "description": "Fixes Errors on the wiiki",
    "manifest_version": 2,
    "name": "Error Fixer",
    "version": "1.3",
    "homepage_url": "http://wiki.tockdom.com/wiki/Special:Contributions/AlmostTWD98",
    "icons": {
        "48": "icons/ATWD98_48.png"
    },
        "content_scripts": [
            {
                "matches": [
                    "*://*.tockdom.com/*"
                ],
                "js": [
                    "fix_error.js",
                    "fix_error2.js",
                    "fix_error3.js"
                ],
                "run_at": "document_idle"
                
            }
        ],
       
    "browser_specific_settings": {
        "gecko": {
            "update_url": "https://raw.githubusercontent.com/Lami-Lemmy/Almost-retardated/main/updates.json"
         }
        }
    }
